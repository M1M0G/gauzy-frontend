{
  "name": "gauzy-frontend",
  "description": "A transparent frontend template to build upon or test stuff.",
  "author": "@lewislbr",
  "repository": "https://github.com/lewislbr/gauzy-frontend.git",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "tsc && webpack-dev-server --mode=development",
    "build": "rm -rf dist && tsc && webpack --mode=production",
    "serve": "serve -s dist -l 7002",
    "lint": "./node_modules/.bin/eslint --ignore-pattern '/node-modules/' --ignore-pattern '/dist/' --ext .ts --ext .tsx .",
    "format": "prettier --write --check \"{,!(dist)/**/}*.{js,jsx,ts,tsx,html,css,json,md}\""
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "compression-webpack-plugin": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fixed-webpack4-html-webpack-inline-chunk-plugin": "^2.0.2",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "serve": "^11.3.0",
    "styled-components": "^5.0.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
